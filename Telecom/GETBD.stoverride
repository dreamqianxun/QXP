name: 电信余量请求体获取
desc: 开启脚本以及mitm等开关，配合电信余量使用

http:
  mitm:
    - czapp.bestpay.com.cn
  script:
    - match: ^https?:\/\/czapp\.bestpay\.com\.cn
      name: Telecom_BD
      type: request
      require-body: true
      timeout: 120


script-providers:
  Telecom_BD:
    url: https://raw.githubusercontent.com/dreamqianxun/QXP/main/Telecom/GetBd.js
    interval: 86400
